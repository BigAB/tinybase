    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rollup": "^3.8.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-gzip": "^3.1.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-prettier": "^3.0.0",
    "tinydocs": "^0.1.23",
    "tmp": "^0.2.1",
    "ts-unused-exports": "^9.0.0",
    "typescript": "4.9.4"
  },
